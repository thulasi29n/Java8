	public static void main(String[] args) {
		
		List<Person> persons = new ArrayList<>();
		persons.add(new Person(23, "John", "USA"));
		persons.add(new Person(23, "Smith", "UK"));
		persons.add(new Person(23, "Ram", "India"));
		persons.add(new Person(23, "Anil", "India"));
		persons.add(new Person(23, "Shoib", "Pakistan"));
		
		
	}

