package in.ashokit;

public class Demo {

	public static void main(String[] args) {
		MyInterface interf = new MyInterface() {
			@Override
			public void sayHi() {
				System.out.println("Hiiiii....");
			}
		};

		interf.sayHi();
	}

}

@FunctionalInterface
interface MyInterface {
	public void sayHi();
}
