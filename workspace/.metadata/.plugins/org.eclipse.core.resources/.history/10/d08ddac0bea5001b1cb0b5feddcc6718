package in.ashokit;

import java.util.ArrayList;
import java.util.List;

public class Demo1 {

	public static void main(String[] args) {
		List<String> fruits = new ArrayList<>();

		fruits.add("Mango");
		fruits.add("Apple");
		fruits.add("Grape");
		fruits.add("Banana");

		for (String fruit : fruits) {
			System.out.println(fruit);
		}

	}

}
