package in.ashok;

import java.util.function.BiConsumer;

public class BiConsumerDemo1 {

	public static void main(String[] args) {
		BiConsumer<String, String> bp = (s1, s2) -> System.out.println(s1 + s2);
		bp.accept("Ashok", "IT");
		
		BiConsumer<Integer, Integer> bp = (i1, i2) - > System.out.println(i1+i2);
	}

}
