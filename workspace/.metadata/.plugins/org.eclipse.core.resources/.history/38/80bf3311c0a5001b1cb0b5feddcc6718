package in.ashokit;

public class Demo2 {

	public static void m2() {
		System.out.println("This is m2() method....");
	}

	public static void main(String[] args) {

		() - > System.out.println("This is m2() method...");
		
		
		MyInterface i = Demo2::m2;
		i.m1();

	}

}

interface MyInterface {
	public void m1();
}
